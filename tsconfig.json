{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@Actions": [
        "./src/Actions"
      ],
      "@Actions/*": [
        "./src/Actions/*"
      ],
      "@Definitions": [
        "./src/Definitions"
      ],
      "@Definitions/*": [
        "./src/Definitions/*"
      ],
      "@Layouts": [
        "./Layouts"
      ],
      "@Layouts/*": [
        "./Layouts/*"
      ],
      "@Client": [
        "./src/Client"
      ],
      "@Client/*": [
        "./src/Client/*"
      ],
      "@Components/*": [
        "./src/Components/*"
      ],
      "@Components": [
        "./src/Components"
      ],
      "@Interfaces": [
        "./src/Interfaces"
      ],
      "@Interfaces/*": [
        "./src/Interfaces/*"
      ],
      "@Models": [
        "./src/Models"
      ],
      "@Models/*": [
        "./src/Models/*"
      ],
      "@Redux": [
        "./src/Redux"
      ],
      "@Redux/*": [
        "./src/Redux/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
